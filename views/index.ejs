                                <a class="nav-link" href="/modelos?patreon=<%= categoria.categoria %>">
                                    <%= categoria.categoria %>
                                </a>
                            </li>
                            <% }) %>
                    </ul>
                    <li class="nav-item">
                        <a class="nav-link" href="/aboutus">ABOUT US</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/auth/login">LOGIN</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

